{
  "title": "Java笔试面试知识整理",
  "author": "guoyang",
  "plugins": [
    "-lunr",
    "-search",
    "search-pro",
    "-sharing",
    "ga",
    "prism",
    "-highlight",
    "anchor-navigation-ex",
    "page-footer-ex",
    "splitter",
    "todo",
    "copy-code-button",
    "expandable-chapters-small",
    "mathjax@https://github.com/OrgVue/gitbook-plugin-mathjax.git#speech-fix"
  ],
  "pluginsConfig": {
    "search-pro": {
      "cutWordLib": "nodejieba",
      "defineWord" : ["Gitbook Use"]
    },
    "anchor-navigation-ex": {
      "showLevel": false
    },
    "ga": {
      "token": "UA-131910384-3"
    },
    "prism": {
      "ignore": [
        "mermaid",
        "eval-js"
      ],
      "css": [
        "prismjs/themes/prism-solarizedlight.css"
      ]
    },
    "page-footer-ex": {
      "copyright": "使用[知识共享 署名-相同方式共享 4.0协议](https://creativecommons.org/licenses/by-sa/4.0/)发布",
      "markdown": true,
      "update_label": "此页面修订于：",
      "update_format": "YYYY-MM-DD HH:mm:ss"
    },
    "mathjax":{
      "forceSVG": false
    }
  }
}